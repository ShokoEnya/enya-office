<%= form_with model: @information, method: :post, class: 'form', local: true do |f| %>
  <div class="form-input">
    <%= f.text_field :title, class: 'info-title', placeholder: 'タイトル' %>
    <%= f.text_area :text, class: 'info-text', placeholder: '「お知らせ」内容' %>
  </div>
  <%= f.submit '送信', class: 'form-submit' %>
<% end %>
